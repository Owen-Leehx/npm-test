{
    "printWidth": 120,
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "semi": false,
    "trailingComma": "none",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "avoid",
    "endOfLine": "auto",
    "quoteProps": "as-needed",
    "jsxSingleQuote": true,
    "overrides": [
        {
          "files": ".prettierrc",
          "options": { "parser": "json" }
        }
      ]
  }
  